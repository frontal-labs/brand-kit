{
  "name": "@frontal/brand-kit",
  "description": "Frontal Brand Kit",
  "version": "0.0.2",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf -- node_modules .next .turbo .cache dist",
    "changeset": "changeset",
    "type-check": "tsc --noEmit --emitDeclarationOnly false",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "format": "biome format --write",
    "release": "changeset tag && changeset github release"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md",
    "CONTRIBUTING.md"
  ],
  "dependencies": {
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.7",
    "@biomejs/biome": "1.9.4",
    "@types/node": "^20.17.47",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.0",
    "jsdom": "^26.1.0",
    "tslib": "^2.2.0",
    "tsx": "^4.7.1",
    "typescript": "^5.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/frontal-labs/brand-kit"
  },
  "keywords": [
    "brand",
    "assets",
    "design",
    "system",
    "ui"
  ]
}
